<?xml version="1.0" encoding="utf-8"?>
<android.support.v7.widget.CardView
android:id="@+id/cardView"
android:focusable="true"
android:clickable="true"
android:layout_width="fill_parent"
android:layout_height="wrap_content"
android:layout_marginLeft="@dimen/spacing_middle"
android:layout_marginTop="@dimen/spacing_medium"
android:layout_marginRight="@dimen/spacing_middle"
android:layout_marginBottom="@dimen/spacing_medium"
android:foreground="?android:selectableItemBackground"
app:cardCornerRadius="10.0dip"
app:cardElevation="2.0dip"
xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:app="http://schemas.android.com/apk/res-auto">
<LinearLayout android:orientation="vertical"
    android:id="@+id/lyt_parent"
    android:background="@color/white"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent">
    <View
        android:background="#ffeceff1"
        android:layout_width="fill_parent"
        android:layout_height="1.0dip" />
    <LinearLayout android:padding="@dimen/spacing_small"
        android:layout_width="fill_parent"
        android:layout_height="0.0dip"
        android:layout_weight="1.0">
        <LinearLayout android:orientation="vertical"
            android:background="@color/white"
            android:padding="5.0dip"
            android:layout_width="0.0dip"
            android:layout_height="wrap_content"
            android:layout_weight="1.0">
            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content">
                <TextView
                    android:textSize="9.0sp"
                    android:gravity="left"
                    android:layout_gravity="left"
                    android:id="@+id/diet_dateTV"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="4.0dip"
                    android:text="@string/hint_register" />
                <TextView
                    android:textSize="9.0sp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/invoice_id" />
                <TextView
                    android:textSize="9.0sp"
                    android:id="@+id/diet_idTV"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="2.0dip"
                    android:text="@string/hint_register" />
                <TextView android:textSize="10.0sp"
                    android:gravity="right"
                    android:layout_gravity="right"
                    android:id="@+id/contactTV"
                    android:visibility="visible"
                    android:layout_width="0.0dip"
                    android:layout_height="wrap_content"
                    android:text="\ 7028259696"
                    android:layout_weight="1.0" />
            </LinearLayout>
            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content">
                <TextView
                    android:textSize="14.0sp"
                    android:textStyle="bold"
                    android:textColor="@android:color/black"
                    android:gravity="left"
                    android:layout_gravity="left"
                    android:id="@+id/nameTV"
                    android:layout_width="0.0dip"
                    android:layout_height="wrap_content"
                    android:text="Atul Patil"
                    android:layout_weight="2.5" />
                <TextView
                    android:textSize="12.0sp"
                    android:textStyle="bold"
                    android:textColor="@color/black"
                    android:gravity="bottom|center|right"
                    android:layout_gravity="bottom|center|right"
                    android:id="@+id/chargesTV"
                    android:layout_width="0.0dip"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="2.0dip"
                    android:text="@string/followup_date"
                    android:layout_weight="1.0" />
            </LinearLayout>
            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content">
                <LinearLayout
                    android:layout_width="0.0dip"
                    android:layout_height="wrap_content"
                    android:layout_weight="1.0">

                    <TextView
                        android:id="@+id/start_to_end_date_TV"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_gravity="start"
                        android:layout_marginLeft="2.0dip"
                        android:gravity="start"
                        android:textSize="9.0sp" />
                </LinearLayout>
                <TextView
                    android:textSize="9.0sp"
                    android:gravity="right"
                    android:layout_gravity="right"
                    android:id="@+id/dietition_TV"
                    android:layout_width="0.0dip"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="2.0dip"
                    android:text="@string/followup_date"
                    android:layout_weight="1.0" />
            </LinearLayout>
            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content">
                <TextView
                    android:textSize="9.0sp"
                    android:gravity="start"
                    android:layout_gravity="start"
                    android:id="@+id/purposeTv"
                    android:layout_width="0.0dip"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="2.0dip"
                    android:text="duration "
                    android:layout_weight="1.0" />
            </LinearLayout>
            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content">
                <LinearLayout
                    android:layout_width="0.0dip"
                    android:layout_height="wrap_content"
                    android:layout_weight="1.0">
                    <TextView
                        android:textSize="9.0sp"
                        android:gravity="center|left"
                        android:layout_gravity="center|left"
                        android:id="@+id/advoiceTV"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="2.0dip" />
                </LinearLayout>

            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
    <View
        android:background="#ffeceff1"
        android:layout_width="fill_parent"
        android:layout_height="1.0dip" />
</LinearLayout>
</android.support.v7.widget.CardView>